{
  "name": "github-contribution-bot",
  "version": "3.0.0",
  "description": "Ultra spam-proof, secure GitHub Auto Commit Bot with OAuth authentication and advanced anti-detection",
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "build": "echo 'No build step required'",
    "deploy": "vercel --prod",
    "test": "echo 'Tests not yet implemented'"
  },
  "keywords": [
    "github",
    "bot",
    "automation",
    "oauth",
    "vercel",
    "serverless",
    "contribution-graph",
    "commits",
    "pwa",
    "rate-limiting",
    "spam-prevention",
    "anti-detection"
  ],
  "author": "CryptoExplor & FarmLabs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CryptoExplor/GitHub-Contribution-Bot"
  },
  "homepage": "https://github.com/CryptoExplor/GitHub-Contribution-Bot#readme",
  "bugs": {
    "url": "https://github.com/CryptoExplor/GitHub-Contribution-Bot/issues"
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "vercel": "^37.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "vercel": {
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.{html,js,json,css}",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
}
